{
  "name": "metube",
  "version": "1.3.2",
  "live_version": "1.0.0",
  "author": "kstory8715 <kstory8715@gmail.com>",
  "description": "An electron-vue project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "MeTube",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "publish": [
      {
        "provider": "generic"
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/logo.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "@fortawesome/vue-fontawesome": "^0.1.3",
    "axios": "^0.16.1",
    "axios-jsonp-pro": "^1.1.4",
    "debug": "^4.1.0",
    "electron-disable-file-drop": "^1.0.2",
    "electron-google-analytics": "^0.1.0",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.2.3",
    "element-ui": "^2.4.7",
    "expose-loader": "^0.7.5",
    "googleapis": "^34.0.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-adapter-idb": "^7.0.0",
    "pouchdb-browser": "^7.0.0",
    "pouchdb-core": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "pouchdb-mapreduce": "^7.0.0",
    "pouchdb-replication": "^7.0.0",
    "querystring": "^0.2.0",
    "request": "^2.88.0",
    "vue": "^2.5.17",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.3.1",
    "vue-js-modal": "^1.3.16",
    "vue-js-toggle-button": "^1.3.0",
    "vue-marquee-text-component": "^1.1.1",
    "vue-niege": "^0.1.1",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vuedraggable": "^2.17.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/devtools": "^1.1.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.10",
    "electron-builder": "^20.34.0",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-rebuild": "latest",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.11.1",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.0",
    "webpack-hot-middleware": "^2.22.0"
  }
}
